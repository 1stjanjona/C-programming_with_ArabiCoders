#include <stdio.h>
#include <stdlib.h>

int main()
{
    int array_1[5] = {1, 3, 9, 5, 7};
    int array_2[5];
    int i;

    for (int i = 0; i < 5; i++)
    {
        array_2[i] = array_1[i];
    }
    printf("The new array is: {%d, %d, %d, %d, %d}", &array_2[0], &array_2[1], &array_2[2], &array_2[3], &array_2[4]);
    return 0;
}
